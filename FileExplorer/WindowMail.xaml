<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FileExplorer"
        xmlns:Wpf="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf" x:Class="FileExplorer.WindowMail"
        mc:Ignorable="d"
        Title="Confirmación" Height="1080" Width="1920" WindowState="Maximized">
    <Grid Margin="0,0,0,-6">
        <Grid.RowDefinitions>
            <RowDefinition Height="108*"/>
            <RowDefinition Height="127*"/>
        </Grid.RowDefinitions>
        <Canvas Grid.RowSpan="2" Margin="0,0,0,10">
            <Canvas.Background>
                <ImageBrush ImageSource="/image/bwall.jpg"/>
            </Canvas.Background>
            <Wpf:WebView2 x:Name="wb" Height="1080" Width="1920"></Wpf:WebView2>
        </Canvas>
    </Grid>
</Window>
